{
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@eslint/eslintrc": "^3.1.0",
    "babel-loader": "^9.1.3",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.1",
    "prettier": "^3.3.2"
  },
  "scripts": {
    "lint": "eslint django/frontend/**/*.js solidity/**/*.js",
    "lint:fix": "eslint --fix 'django/frontend/**/*.{js,jsx}' 'solidity/**/*.js'",
    "start": "webpack serve --hot --config webpack.dev.js",
    "build": "webpack build --config webpack.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky"
  },
  "lint-staged": {
    "django/frontend/**/*.{js,jsx}": [
      "prettier --write --loglevel=error",
      "eslint --fix --quiet"
    ],
    "{public,django/frontend}/**/*.{html,gql,graphql,json}": [
      "prettier --write --loglevel=error"
    ],
    "solidity/**/*.js": [
      "prettier --write --loglevel=error",
      "eslint --fix --quiet"
    ],
    "solidity/**/*.sol": [
      "prettier --write --plugin=prettier-plugin-solidity --loglevel=error",
      "solhint"
    ]
  },
  "dependencies": {
    "babel": "^6.23.0",
    "husky": "^9.1.4",
    "js-cookie": "^3.0.5",
    "lint-staged": "^15.2.7",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^5.0.1"
  }
}
